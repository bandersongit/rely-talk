{
  "name": "rely-talk",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "refmterr dune build --profile=release",
    "install": [
      "esy-installer rely-talk.install",
      "esy-installer rely-talk-tests.install"
    ]
  },
  "scripts": {
    "test": "esy x RunTests.exe",

    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "~4.6.0",
    "@reason-native/rely": "^1.2.0"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*"
  },
  "resolutions": {
    "@opam/ocaml-migrate-parsetree": "ocaml-ppx/ocaml-migrate-parsetree:ocaml-migrate-parsetree.opam#a5be5e1b94f02c667ab354c2e6b55baedeb30187"
  }
}
